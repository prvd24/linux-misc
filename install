## install .rpm
sudo dnf install ./package.rpm
## alternatively
sudo rpm --install  --excludepath=/usr/lib/.build-id/ ./package.rpm

sudo dnf -y install ./master-pdf-editor-5.9.40-qt5.x86_64.rpm
sudo perl -pi -e 's/(\xE8...\xFF)\x88(..\xBF\x30)/$1\xFE$2/g' /opt/master-pdf-editor-5/masterpdfeditor5

sudo dnf install fedora-workstation-repositories
# sudo dnf install google-chrome-stable
sudo dnf install powertop wireguard-tools aria2 git p7zip p7zip-plugins python3-pip

sudo powertop --auto-tune ; sudo systemctl enable powertop.service

